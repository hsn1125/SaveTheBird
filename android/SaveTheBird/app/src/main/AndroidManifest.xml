        </activity>
        <activity android:name=".HomeActivity">
            >
        </activity>
        <activity android:name=".GameActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".CharacterSelectActivity"></activity>
    </application>

</manifest>